{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build List",
            "type": "shell",
            "command": "g++",
            "args": [
                "Lab3.cpp",
                "Node.cpp",
                "LinkedList.cpp",
                "-o",
                "List"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$gcc"
        },
        {
            "label": "clean",
            "type": "shell",
            "command": "rm",
            "args": ["-rf", "*.o", "List"],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        }
    ]
}
